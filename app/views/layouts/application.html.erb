<!DOCTYPE html>
<html>
  <head>
    <title>GemaweekBase</title>
    <%= stylesheet_link_tag 'application' %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id='container'>
      <% if notice %>
        <p class='success'><%= notice %></p>
      <% elsif alert %>
        <p class='error'><%= alert %></p>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
